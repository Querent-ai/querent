{
	"rian-main-capability": {
		"identifier": "rian-main-capability",
		"description": "Capability for the core rian application",
		"remote": {
			"urls": ["https://accounts.google.com", "https://www.google.com", "https://www.youtube.com"]
		},
		"local": true,
		"windows": ["main", "google-oauth"],
		"permissions": [
			"dialog:allow-ask",
			"dialog:allow-save",
			"dialog:allow-open",
			"dialog:allow-message",
			"dialog:allow-confirm",
			"shell:allow-open",
			"fs:default",
			"fs:allow-app-write",
			"fs:allow-app-write-recursive",
			"fs:allow-appcache-write",
			"fs:allow-appcache-write-recursive",
			"core:webview:allow-create-webview-window",
			"shell:default",
			"core:event:allow-listen",
			"notification:allow-is-permission-granted",
			"notification:default",
			"core:event:allow-unlisten"
		]
	}
}
